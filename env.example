# Vercel Deployment
VERCEL_ORG_ID=your_vercel_org_id_here
VERCEL_PROJECT_ID=your_vercel_project_id_here
# CI-only secret (set in provider secrets, not in local .env committed to VCS)
VERCEL_TOKEN=your_vercel_token_here

# Docker Hub
# Docker Hub (CI-only; do NOT commit real values, and never prefix with NEXT_PUBLIC)
DOCKERHUB_USERNAME=your_docker_username_here
DOCKERHUB_TOKEN=your_docker_pat_here

# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id_here

# App Configuration
NEXT_PUBLIC_APP_NAME=LotteryApp
NEXT_PUBLIC_APP_URL=https://your-domain.vercel.app
